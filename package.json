{
  "name": "TheDuchy-nest-server",
  "version": "0.0.2",
  "engines": {
    "node": "16.17.0",
    "npm": "7.20.3"
  },
  "description": "",
  "author": "dangloi",
  "private": false,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "console": "ts-node -r tsconfig-paths/register src/console.ts",
    "seed:dev": "npm run console:dev seed",
    "seed:prod": "npm run console seed",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "heroku-postbuild": "npm run build && npm run migrate:up",
    "gcp-build": "npm run build",
    "create-env": "printenv > .env",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node ./dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:cov:watch": "jest --coverage --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.config.js",
    "test:e2e:watch": "jest --config ./jest-e2e.config.js --watch",
    "typeorm": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/database.ts",
    "migrate:create": "npm typeorm -- migration:create -d src/migrations -n",
    "migrate:generate": "npm run typeorm -- migration:generate -d src/migrations -p -n",
    "migrate:up": "npm run typeorm migration:run",
    "migrate:down": "npm run typeorm migration:revert"
  },
  "dependencies": {
